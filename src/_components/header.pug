header.header-wrap
  .container
    .row
      .col-12
        .header
          .header-left
            .header-logo
              a(href="javascript:void(0);").header-logo-link
                img(src='../img/Logo.svg' alt='лого', title='').logo
            .header-menu
              ul.header-menu_list
                li.header-menu__item
                  a(href="javascript:void(0);").header-menu__link О нас
                li.header-menu__item
                  a(href="javascript:void(0);").header-menu__link Услуги
